	border-left: 1px solid #444;
	border-right: 1px solid #777;
	float: left;
	height: 24px;
	margin: 2px 1px;
}

.datagrid .datagrid-pager {
	border-style: solid;
	border-width: 1px 0 0 0;
	display: block;
	margin: 0;
}

.datagrid .datagrid-pager-top { border-width: 0 0 1px 0; }

.datagrid-header {
	border-style: solid;
	border-width: 0 0 1px 0;
	cursor: default;
	overflow: hidden;
}

.datagrid-header-inner {
	float: left;
	width: 10000px;
}

.datagrid-header-row,
.datagrid-row { height: 25px; }

.datagrid-header td,
.datagrid-body td,
.datagrid-footer td {
	border-style: dotted;
	border-width: 0 1px 1px 0;
	margin: 0;
	padding: 0;
}

.datagrid-cell,
.datagrid-cell-group,
.datagrid-header-rownumber,
.datagrid-cell-rownumber {
	font-size: 12px;
	height: 18px;
	line-height: 18px;
	margin: 0;
	overflow: hidden;
	padding: 0 4px;
	white-space: nowrap;
	word-wrap: normal;
}

.datagrid-header .datagrid-cell { height: auto; }

.datagrid-header .datagrid-cell span { font-size: 12px; }

.datagrid-cell-group {
	text-align: center;
	text-overflow: ellipsis;
}

.datagrid-header-rownumber,
.datagrid-cell-rownumber {
	margin: 0;
	padding: 0;
	text-align: center;
	width: 30px;
}

.datagrid-body {
	margin: 0;
	overflow: auto;
	padding: 0;
	zoom: 1;
}

.datagrid-view1 .datagrid-body-inner { padding-bottom: 20px; }

.datagrid-view1 .datagrid-body { overflow: hidden; }

.datagrid-footer { overflow: hidden; }

.datagrid-footer-inner {
	border-style: solid;
	border-width: 1px 0 0 0;
	float: left;
	width: 10000px;
}

.datagrid-row-editing .datagrid-cell { height: auto; }

.datagrid-header-check,
