﻿@model MVCSchooldbDemo.Models.Data.StudentInfo
@{
    Layout = "~/Views/Shared/LayoutEditor/_LayoutEditorDetails.cshtml";
}
<script>
    $(function() {
        ShowInfo();
        ReadonlyControls('#Sno,#Sname,#Ssex,#Sage,#Sdept');
    });

    function ShowInfo() {
        $('#Sno').val('@Model.Sno');
        $('#Sname').val('@Model.Sname');
        $('#Ssex').val('@Model.Ssex');
        $('#Sage').val('@Model.Sage');
        $('#Sdept').val('@Model.Sdept');
    }
</script>

@Html.Partial("_PartialEditor")